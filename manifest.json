{
    "name": "n8n",
    "id": "n8n",
    "packaging_format": 1,
    "description": {
        "en": "",
        "fr": ""
    },
    "version": "1.8.13~ynh1",
    "url": "",
    "license": "Apache-2.0",
    "maintainer": {
        "name": "eric_G"
    },
    "requirements": {
        "yunohost": ">= 4.2.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/n8n",
                "default": "/n8n"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "help": {
                    "en": "If enabled, Etherpad will be accessible by people who do not have an account. This can be changed later via the webadmin.",
                    "fr": "Si cette case est cochée, Etherpad sera accessible aux personnes n’ayant pas de compte. Vous pourrez changer ceci plus tard via la webadmin."
            },
                "default": true
            }
        ]
    }
}
